{
  "name": "VeloX",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "ci": "yarn install --frozen-lockfile",
    "ci:test": "yarn nx test velo-x",
    "ci:rnModules": "yarn install --frozen-lockfile",
    "ci:pods": "cd ./ios && pod install && cd ..",
    "fastlane:installAndroid": "cd ./android && bundle install && cd ..",
    "fastlane:installIos": "cd ./ios && bundle install && cd ..",
    "fastlane:updateAndroid": "cd ./android && bundle update fastlane && cd ..",
    "fastlane:updateIos": "cd ./ios && bundle update fastlane && cd ..",
    "fastlane:buildAndroid": "cd ./android && bundle exec fastlane android build && cd ..",
    "fastlane:buildIos": "cd ./ios && bundle exec fastlane ios build && cd .."
  },
